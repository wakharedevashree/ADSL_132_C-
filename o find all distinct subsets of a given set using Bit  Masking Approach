#include <bits/stdc++.h>
using namespace std;

int main() {
    int n;
    cout << "Enter number of elements: ";
    cin >> n;

    int arr[100]; 
    cout << "Enter elements: ";
    for (int i = 0; i < n; i++)
        cin >> arr[i];

  
    sort(arr, arr + n);

    int total = 1 << n; 

    cout << "\nAll distinct subsets are:\n";

 
    string printed[1000];
    int printedCount = 0;

    for (int mask = 0; mask < total; mask++) {
        string subset = "";

        for (int i = 0; i < n; i++) {
            if (mask & (1 << i)) { // if i-th bit is set
                subset += to_string(arr[i]) + " ";
            }
        }

       
        bool alreadyPrinted = false;
        for (int j = 0; j < printedCount; j++) {
            if (printed[j] == subset) {
                alreadyPrinted = true;
                break;
            }
        }

        if (!alreadyPrinted) {
            printed[printedCount++] = subset;
            cout << "{ " << subset << "}\n";
        }
    }

    return 0;
}
